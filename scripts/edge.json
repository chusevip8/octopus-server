[
  {
    "app": {
      "name": "小红书",
      "packageName": "com.xingin.xhs",
      "intent": {
        "scheme": "xhsdiscover://profile"
      }
    },
    "activities": [
      {
        "title": "个人页面",
        "className": "com.xingin.xhs.index.v2.IndexActivityV2",
        "views": [
          {
            "title": "账号",
            "type": "view",
            "value": "",
            "match": {
              "by": "class",
              "value": "android.widget.TextView",
              "index": 0
            },
            "event": {
              "yesEvent": {
                "type": "extract",
                "value": "collect://map/text/commentReplier",
                "next": "1001"
              }
            }
          }
        ]
      }
    ]
  },
  {
    "app": {
      "name": "Octopus 浏览器",
      "packageName": "com.octopus.browser",
      "intent": {
        "scheme": "${postLink}"
      }
    },
    "activities": [
      {
        "title": "启动页面",
        "className": "com.octopus.browser.activity.MainActivity",
        "views": [
          {
            "title": "打开看看",
            "type": "view",
            "value": "",
            "match": {
              "by": "text",
              "value": "打开看看",
              "index": 0
            },
            "event": {
              "yesEvent": {
                "type": "click",
                "value": "",
                "next": "1001"
              }
            }
          }
        ]
      }
    ]
  },
  {
    "app": {
      "name": "小红书",
      "packageName": "com.xingin.xhs"
    },
    "activities": [
      {
        "title": "启动页面",
        "className": "com.octopus.start.Activity",
        "views": [
          {
            "title": "等待-0",
            "type": "view",
            "value": "",
            "event": {
              "yesEvent": {
                "type": "wait",
                "value": "2000",
                "next": "1003"
              }
            }
          }
        ]
      },
      {
        "title": "详情页面",
        "className": "com.xingin.matrix.notedetail.NoteDetailActivity",
        "views": [
          {
            "title": "作者-0",
            "type": "view",
            "value": "",
            "match": {
              "by": "class",
              "value": "android.widget.Button",
              "index": 1
            },
            "condition": {
              "type": "contains",
              "match": "des",
              "value": "作者,"
            },
            "event": {
              "yesEvent": {
                "type": "extract",
                "value": "collect://map/desc/poster",
                "next": ""
              }
            }
          },
          {
            "title": "文章标题/描述-1",
            "type": "view",
            "value": "",
            "match": {
              "by": "class",
              "value": "android.widget.TextView",
              "index": 1
            },
            "event": {
              "yesEvent": {
                "type": "extract",
                "value": "collect://map/text/postTitle",
                "next": ""
              }
            }
          },
          {
            "title": "评论按钮-2",
            "type": "view",
            "value": "",
            "match": {
              "by": "class",
              "value": "android.view.ViewGroup",
              "index": 0,
              "deep": "child://3"
            },
            "condition": {
              "type": "equals",
              "match": "des",
              "value": "评论 0"
            },
            "event": {
              "yesEvent": {
                "type": "error",
                "value": "没有评论",
                "next": ""
              },
              "noEvent": {
                "type": "click",
                "value": "",
                "next": ""
              }
            }
          },
          {
            "title": "等待-3",
            "type": "view",
            "value": "",
            "event": {
              "yesEvent": {
                "type": "wait",
                "value": "2000",
                "next": ""
              }
            }
          },
          {
            "title": "更新界面-4",
            "type": "update",
            "value": "",
            "event": {
              "yesEvent": {
                "type": "",
                "value": "",
                "next": ""
              }
            }
          },
          {
            "title": "评论列表-5",
            "type": "list",
            "value": "class://android.widget.TextView,1",
            "match": {
              "by": "class",
              "value": "androidx.recyclerview.widget.RecyclerView",
              "index": 1
            },
            "condition": {
              "type": "equals",
              "match": "text",
              "value": "0"
            },
            "event": {
              "yesEvent": {
                "type": "",
                "value": "",
                "next": "1001"
              },
              "noEvent": {
                "type": "",
                "value": "",
                "next": ""
              }
            }
          },
          {
            "title": "列表项-6",
            "type": "item",
            "value": "5",
            "event": {
              "yesEvent": {
                "type": "",
                "value": "",
                "next": "9"
              },
              "noneEvent": {
                "type": "",
                "value": "",
                "next": ""
              }
            }
          },
          {
            "title": "检测是否到底-7",
            "type": "view",
            "value": "",
            "match": {
              "by": "des",
              "value": "已到底",
              "index": 0
            },
            "event": {
              "yesEvent": {
                "type": "",
                "value": "",
                "next": "1001"
              },
              "noneEvent": {
                "type": "",
                "value": "",
                "next": ""
              }
            }
          },
          {
            "title": "滑动列表-8",
            "type": "view",
            "value": "",
            "event": {
              "yesEvent": {
                "type": "slide",
                "value": "2,0,0.3,0,0.3",
                "next": "3"
              }
            }
          },
          {
            "title": "评论项-9",
            "type": "view",
            "value": "",
            "match": {
              "start": "6",
              "by": "class",
              "value": "android.widget.TextView",
              "index": 1
            },
            "condition": {
              "type": "equals",
              "match": "text",
              "value": "作者"
            },
            "event": {
              "yesEvent": {
                "type": "",
                "value": "",
                "next": "6"
              },
              "noEvent": {
                "type": "",
                "value": "",
                "next": ""
              }
            }
          },
          {
            "title": "评论项-10",
            "type": "view",
            "value": "",
            "match": {
              "start": "6",
              "by": "class",
              "value": "android.widget.TextView",
              "index": 1
            },
            "condition": {
              "type": "contains",
              "match": "text",
              "value": "list://${keyword}"
            },
            "event": {
              "yesEvent": {
                "type": "extract",
                "value": "collect://block/text/content",
                "next": ""
              },
              "noEvent": {
                "type": "",
                "value": "",
                "next": "6"
              }
            }
          },
          {
            "title": "评论作者-11",
            "type": "view",
            "value": "",
            "match": {
              "start": "6",
              "by": "class",
              "value": "android.widget.TextView",
              "index": 0
            },
            "event": {
              "yesEvent": {
                "type": "extract",
                "value": "collect://block/text/commenter",
                "next": ""
              }
            }
          },
          {
            "title": "放入Collect-12",
            "type": "view",
            "value": "",
            "event": {
              "yesEvent": {
                "type": "extract",
                "value": "collect://list/comments",
                "next": ""
              }
            }
          },
          {
            "title": "上传数据-13",
            "type": "output",
            "value": "https://bzy.ai4mobi.com/api/cmtTask/uploadFindComment",
            "event": {
              "yesEvent": {
                "type": "",
                "value": "",
                "next": ""
              }
            }
          },
          {
            "title": "计数-14",
            "type": "count",
            "value": "${cmtCount}",
            "condition": {
              "type": "equals",
              "match": "text",
              "value": "1"
            },
            "event": {
              "yesEvent": {
                "type": "",
                "value": "",
                "next": "1001"
              },
              "noEvent": {
                "type": "",
                "value": "",
                "next": ""
              }
            }
          },
          {
            "title": "清除数据-15",
            "type": "view",
            "value": "",
            "event": {
              "yesEvent": {
                "type": "clean",
                "value": "comments",
                "next": "6"
              }
            }
          }
        ]
      }
    ]
  }
]